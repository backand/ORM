  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "minio",
      "cwd": "${workspaceRoot}/list-s3",
      "program": "${workspaceRoot}/list-s3/minio.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Azure Function List",
      "cwd": "${workspaceRoot}/azure",
      "program": "${workspaceRoot}/azure/get_functions_list.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Azure Invoke Function",
      "cwd": "${workspaceRoot}/azure",
      "program": "${workspaceRoot}/azure/invoke_function.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Azure Tests",
        "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
        "args": [
            "-u",
            "tdd",
            "--timeout",
            "999999",
            "--colors",
            "${workspaceRoot}/test/azure_test.js"
        ],
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "GCP Tests",
        "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
        "args": [
            "-u",
            "tdd",
            "--timeout",
            "999999",
            "--colors",
            "${workspaceRoot}/test/gcp_test.js"
        ],
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "All Tests",
        "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
        "cwd": "${workspaceRoot}",
        "args": [
            "-u",
            "tdd",
            "--timeout",
            "999999",
            "--colors",
            "${workspaceRoot}/test/lambda_test.js"
        ],
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Google Function List",
      "cwd": "${workspaceRoot}/gcp",
      "program": "${workspaceRoot}/gcp/get_functions_list.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "GCP Invoke Function",
      "cwd": "${workspaceRoot}/gcp",
      "program": "${workspaceRoot}/gcp/invoke_function.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "openFaas Function List",
      "cwd": "${workspaceRoot}/openFaas",
      "program": "${workspaceRoot}/openFaas/get_functions_list.js"
    }
  ]
}